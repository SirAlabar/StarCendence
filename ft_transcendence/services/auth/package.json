{
	"name": "auth-service",
	"version": "1.0.0",
	"description": "Auth management microservice",
	"main": "dist/server.js",
	"scripts": {
	  "build": "tsc",
	  "start": "node dist/server.js",
		"dev": "nodemon --watch src --ext ts --exec tsx src/server.ts",
	  "db:generate": "prisma generate",
	  "db:migrate": "prisma migrate dev",
	  "db:seed": "tsx prisma/seed.ts",
	  "prisma:seed": "ts-node prisma/seed.ts"
	},
	"dependencies": {
	  "@fastify/cors": "^8.3.0",
	  "@fastify/helmet": "^11.1.1",
	  "@prisma/client": "^5.0.0",
	  "bcrypt": "^5.1.0",
	  "dotenv": "^16.3.1",
	  "fastify": "^4.21.0",
	  "jsonwebtoken": "^9.0.0",
	  "speakeasy": "^2.0.0",
	  "qrcode": "^1.5.3"
	},
	"devDependencies": {
	  "@types/bcrypt": "^5.0.0",
	  "@types/jsonwebtoken": "^9.0.1",
	  "ts-node": "^10.9.1",
	  "@types/node": "^20.4.5",
	  "@types/qrcode": "^1.5.2",
	  "@typescript-eslint/eslint-plugin": "^6.0.0",
	  "@typescript-eslint/parser": "^6.0.0",
	  "eslint": "^8.45.0",
	  "nodemon": "^3.0.1",
	  "prisma": "^5.22.0",
	  "tsx": "^3.12.7",
	  "typescript": "^5.1.6"
	}
}